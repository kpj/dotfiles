# xbindkeys -k

"xbacklight -inc 10"
    XF86MonBrightnessUp
"xbacklight -dec 10"
    XF86MonBrightnessDown

"amixer set Master 5%+"
    XF86AudioRaiseVolume
"amixer set Master 5%-"
    XF86AudioLowerVolume

"mpv $(xsel)"
    F8
"systemctl suspend"
    XF86Sleep
"i3lock -c 000000"
    Mod4 + g
"scrot -e 'mv $f ~/Sync/screenshots'"
    Print
